            "type": "Slice",
            "fieldset": "Header",
            "description": "Page Header ",
            "icon": "border_color",
            "display": "list",
            "non-repeat": {
              "heading": {
                "type": "StructuredText",
                "config": {
                  "single": "heading1",
                  "label": "Heading"
                }
              },
              "body_text": {
                "type": "StructuredText",
                "config": {
                  "multi": "rtl",
                  "label": "body text"
                }
              },
              "image": {
                "type": "Image",
                "config": {
                  "constraint": {},
                  "thumbnails": [],
                  "label": "image"
                }
              }
            },
            "repeat": {
              "button_link": {
                "type": "Link",
                "config": {
                  "label": "button link",
                  "select": null
                }
              },
              "button_label": {
                "type": "Text",
                "config": {
                  "label": "button label"
                }
              }
            }
          },
          "call_to_action": {
            "type": "Slice",
            "fieldset": "Call to action",
            "description": "Call to action section for midpage or end of page",
            "icon": "arrow_forward",
            "display": "list",
            "non-repeat": {
              "icon_image": {
                "type": "Image",
                "config": {
                  "constraint": {},
                  "thumbnails": [],
                  "label": "Icon Image"
                }
              },
              "paragraph": {
                "type": "StructuredText",
                "config": {
                  "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,o-list-item",
                  "label": "Paragraph"
                }
              }
            },
            "repeat": {
              "button_link": {
                "type": "Link",
                "config": {
                  "label": "Button Link",
                  "select": null
                }
              },
              "button_label": {
                "type": "Text",
                "config": {
                  "label": "Button Label",
                  "placeholder": "Text for button"
                }
              }
            }
          },
          "bible_verse": {
            "type": "Slice",
            "fieldset": "Bible Verse",
            "description": "Bible verse ",
            "icon": "library_books",
            "display": "list",
            "non-repeat": {
              "verse": {
                "type": "StructuredText",
                "config": {
                  "single": "paragraph",
                  "label": "Verse",
                  "placeholder": "For God so loved the world...."
                }
              },
              "verse_location": {
                "type": "StructuredText",
                "config": {
                  "single": "paragraph",
                  "label": "Verse location",
                  "placeholder": "John: 3:16"
                }
              }
            },
            "repeat": {}
          },
          "blog_content": {
            "type": "Slice",
            "fieldset": "Blog Content",
