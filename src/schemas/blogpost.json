            "icon": "border_color",
            "display": "list",
            "non-repeat": {
              "main_text": {
                "type": "StructuredText",
                "config": {
                  "multi": "paragraph,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,list-item,o-list-item,rtl",
